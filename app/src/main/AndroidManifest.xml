<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dell.project">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashAcivity"
            android:theme="@style/Theme.Design.Light.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".NewMember" android:label="@string/title_activity_new_student"/>
        <activity android:name=".MainActivity" android:label="@string/title_activity_main"/>
        <activity android:name=".FindActivity" android:label="@string/title_activity_find"/>
        <activity android:name=".MemberinfoActivity" android:label="@string/title_activity_member_info"/>

    </application>

</manifest>